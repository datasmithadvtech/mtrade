          <stringProp name="filename"></stringProp>
          <stringProp name="script">import java.text.SimpleDateFormat;
import java.util.Locale;

String[] CURR = new String[] {&quot;CZK&quot;, &quot;EUR&quot;, &quot;GBP&quot;, &quot;USD&quot;, &quot;JPY&quot;};
String[] STATES = new String[] {&quot;CZ&quot;, &quot;DE&quot;, &quot;PL&quot;, &quot;IE&quot;, &quot;GB&quot;, &quot;US&quot;, &quot;JP&quot;, &quot;IS&quot;, &quot;DK&quot;, &quot;NL&quot;};

Random random = new Random();
Integer userId = random.nextInt(10000);
String currencyFrom = CURR[random.nextInt(CURR.length)];
String currencyTo = CURR[random.nextInt(CURR.length)];
Float amountSell = random.nextFloat() * 100; 
Float amountBuy = random.nextFloat() * 100; 
Float rate = random.nextFloat(); 
Date timePlaced = new Date(random.nextLong());
SimpleDateFormat sdf = new SimpleDateFormat(&quot;dd-MMM-yy hh:mm:ss&quot;, Locale.US);
String country = STATES[random.nextInt(STATES.length)];

vars.put(&quot;json&quot;, &quot;{\&quot;userId\&quot;: \&quot;&quot;+userId+&quot;\&quot;, \&quot;currencyFrom\&quot;: \&quot;&quot;+currencyFrom+&quot;\&quot;, \&quot;currencyTo\&quot;: \&quot;&quot;+currencyTo+&quot;\&quot;, \&quot;amountSell\&quot;: &quot;+amountSell+&quot;, \&quot;amountBuy\&quot;: &quot;+amountBuy+&quot;, \&quot;rate\&quot;: &quot;+rate+&quot;, \&quot;timePlaced\&quot; : \&quot;&quot;+sdf.format(timePlaced)+&quot;\&quot;, \&quot;originatingCountry\&quot; : \&quot;&quot;+country+&quot;\&quot;}&quot;);
log(vars);
</stringProp>
